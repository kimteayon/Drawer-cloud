.input-section{
  border-bottom: 1px solid #ccc;
  margin: 20px 10px;
  position: relative;
  height: 30px;
}
.input{
  position: absolute;
  font-size: 14px;
  width: 80%;
}
.ui-pushdymics-text {
  margin-top: 5px;
  padding-top: 5px;
  width: 100%;
  border-bottom: 1px solid #e5e5e5;
  background-color: #fff;
}
.text-max{
  position: absolute;
  right: 0px;
  font-size: 12px;
  color:#666;
  bottom: 2px;
}
.inputbtn{
    width: 100%;
    height: 30px;
}
.inputbtn button{
  width: 90%;
  background: #3399ff;
  font-size: 14px;
  line-height: 30px;
  color: #fff;
}
.label-item{
  border: 1px solid #ccc;
  padding: 5px 10px;
  border-radius: 20px;
  margin-bottom: 20px;
  margin-right: 20px;
}
.container-label{
  display: flex;
  flex-wrap:wrap-reverse;
  justify-content:space-around;
  margin-top: 30px;
  align-content:space-around;
}