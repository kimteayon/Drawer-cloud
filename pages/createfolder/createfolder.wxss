/* pages/createfolder/createfolder.wxss */
.input-section {
  border-bottom: 1px solid #ccc;
  margin: 20px 10px;
  position: relative;
  height: 30px;
}
.input {
  position: absolute;
  font-size: 14px;
  width: 80%;
}
.folder-description-icon{
  width: 16px;
  height: 16px;
  position: absolute;
  left: 10px;
}
.ui-pushdymics-text {
  padding-top: 20px;
  width: 100%;
  background-color: #fff;

}

.text-max {
  position: absolute;
  right: 0px;
  font-size: 12px;
  color: #666;
  bottom: 2px;
}

.inputbtn {
  width: 100%;
  height: 30px;
}

.inputbtn button {
  width: 90%;
  background: #39f;
  font-size: 14px;
  line-height: 30px;
  color: #fff;
}

.label-item {
  border: 1px solid #ccc;
  padding: 5px 10px;
  border-radius: 20px;
  margin-bottom: 20px;
  margin-right: 20px;
}

.container-label {
  display: flex;
  flex-wrap: wrap-reverse;
  justify-content: space-around;
  margin-top: 30px;
  align-content: space-around;
}
.noclick{
 background: #ccc; 
}
.catalog-list-megia {
  display: inline-flex;
  width: 100%;
  height: 100%;
}
.catalog-deletephoto-icon {
  position: absolute;
  display: block;
  width: 20px;
  height: 20px;
  padding: 5px;
  top: -4px;
  right: -3px;
}
page {
  height: 100%;
}
.catalog-item {
  width: 25%;
  height: 168rpx;
  display: inline-flex;
  border: 1px solid #fff;
  box-sizing: border-box;
  position: relative;
}
.catalog-plus-img {
  width: 68rpx;
  height: 68rpx;
  margin: auto auto;
}
.catalog-item-plus {
  width: 25%;
  height: 168rpx;
  display: inline-flex;
  border: 1px solid #e5e5e5;
  box-sizing: border-box;
}

